{
	"name": "storyblok-svelte-boilerplate",
	"description": "An Sapper boilerplate to integrate with Storyblok Headless CMS",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev",
		"build": "sapper build",
		"start": "node __sapper__/build",
		"export": "sapper export --entry editor",
		"deploy": "sapper export --entry editor && ntl deploy --prod",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run"
	},
	"dependencies": {
		"bricklayer": "^0.4.3",
		"compression": "^1.7.1",
		"lodash": "^4.17.21",
		"polka": "0.5.2",
		"sirv": "^1.0.1",
		"storyblok-js-client": "^2.5.1",
		"tua-body-scroll-lock": "^1.2.1",
		"webfontloader": "^1.6.28"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5",
		"sapper": "^0.27.16",
		"svelte": "^3.24.0",
		"svelte-loader": "^2.9.0",
		"webpack": "^4.43.0"
	}
}
